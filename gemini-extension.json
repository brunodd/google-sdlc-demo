{
  "name": "forgotten-prompt-extension",
  "version": "1.0.0",
  "mcpServers": {
    "nodeServer": {
      "command": "node",
      "args": ["${extensionPath}${/}dist${/}example.js"],
      "cwd": "${extensionPath}"
    }
  },
  "commands": {
    "start": {
      "path": "commands/start.toml"
    },
    "action": {
      "path": "commands/action.toml"
    },
    "list": {
      "path": "commands/list.toml"
    }
  }
}
